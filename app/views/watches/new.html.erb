<div class="container">
  <div>
    <h1> Add a new watch</h1>
  </div>

  <div>
    <%= simple_form_for(@watch) do |f| %>
      <%= f.input :brand %>
      <%= f.input :model %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :photo, as: :file %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
